<service>
	<id>vacationcontrollers.exe</id>
	<name>Vacation-controllers</name>
	<description>Database of employe`s vacations</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>D:\git\vacation-controller\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\git\vacation-controller\api\apiserver.js</argument>
	<argument>--log</argument>
	<argument>Vacation-controllers wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\git\vacation-controller</workingdirectory>
</service>